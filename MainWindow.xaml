        <Button Content="Start" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="100" Click="Start_Click"/>
        <TextBox x:Name="ResultTextBox" HorizontalAlignment="Left" Height="200" Margin="10,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="480"/>
    </Grid>
</Window>
